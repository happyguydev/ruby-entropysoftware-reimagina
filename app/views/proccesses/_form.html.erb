<%= simple_form_for([@order,@proccess]) do |f| %>
<%= f.error_notification %>

<div class="form-group col-md-3">
  <label>Proceso</label>
  <%= f.association :proc_type,:label_method => :nombre, :label => false, input_html: { class: 'chosen-select' } %></div>

<div class="form-group col-md-2">
  <label>Fecha activaci√≥n</label>
  <%= f.input :fecha, :as => :date_picker , :label => false, input_html: {class: 'form-control'} %>
</div>

<div class="form-group col-md-2">
  <label>Monto</label>
  <%= f.input :monto, :label => false, input_html: {class: 'form-control'} %>
</div>

<div class="form-group col-md-12">
  <%= f.button :submit , 'Enviar'%>
</div>

<% end %>
