<section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="box col-sm-12 col-md-12 col-lg-12">
        <div class="box-header">
          <h3 class="box-title">Tipos de proceso</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
          <div class="dataTables_wrapper form-inline dt-bootstrap" id="example2_wrapper">
            <div class="row">
              <div class="col-sm-6"></div>
              <div class="col-sm-6"></div>
            </div>
            <div class="row">
              <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <table id='proc_types-table'  class="table table-bordered table-hover dataTable" data-source='<%= proc_types_path(format: :json) %>'>
                  <thead>
                    <tr>
                      <th>Nombre <br /></th>
                      <th>Tipo <br /></th>
                      <th>Orden <br /></th>
                      <th>Opciones <br /></th>
                    </tr>
                  </thead>
                  <tbody>
                  </tbody>
                </table>
              </div>
            </div>
            <div class="row"></div>
            <div class="col-sm-7"></div>
          </div>
        </div>
      </div>
      <!-- /.box-body -->
    </div>
  </div>
</section>


<div class="col-md-4">
  <div class="box box-default collapsed-box">
    <div class="box-header with-border">
      <h3 class="box-title">Crear Tipo de Proceso</h3>
      <div class="box-tools pull-right">
        <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-plus"></i>
        </button>
      </div>
      <!-- /.box-tools -->
    </div>
    <!-- /.box-header -->
    <div style="display: none;" class="box-body">
      <%= render 'form' %>
    </div>
    <!-- /.box-body -->
  </div>
  <!-- /.box -->
</div>
