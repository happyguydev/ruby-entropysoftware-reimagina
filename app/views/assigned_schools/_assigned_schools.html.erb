<div class='col-md-12'>
  <% if @assigned_schools.present? %>
    <% @assigned_schools.each do |assigned| %>
      <a href="<%= '/schools/' + assigned.try(:school).try(:id).try(:to_s) %>">
        <div class='col-md-3 assigned-school'>
          Entrar a establecimiento <br>
          <%= assigned.try(:school).try(:nombre) %>
        </div>
      </a>
    <% end %>
  <% else %>
    Sin establecimiento asignado
  <% end %>
</div>
